<Window x:Class="forum_system.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:forum_system"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="Login" Content="Login" HorizontalAlignment="Left" Margin="104,205,0,0" VerticalAlignment="Top" Width="123" Click ="Login_Click"/>
        <Button x:Name="Register" Content="Register" HorizontalAlignment="Left" Margin="294,205,0,0" VerticalAlignment="Top" Width="119" Click="Register_Click"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="63,26,0,0" TextWrapping="Wrap" Text="Welcome to Static forum" VerticalAlignment="Top" Height="34" Width="350"/>

    </Grid>
</Window>
